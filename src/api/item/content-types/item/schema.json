{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Title": {
      "type": "string"
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Breakfast",
        "Lunch",
        "Snacks",
        "Dinner"
      ]
    },
    "Servings": {
      "type": "integer"
    },
    "Expected_time_in_minutes": {
      "type": "decimal"
    },
    "trending": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::trending.trending",
      "inversedBy": "items"
    },
    "item_description": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::item-description.item-description",
      "inversedBy": "item"
    }
  }
}
